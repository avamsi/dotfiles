# Options -- http://zsh.sourceforge.net/Doc/Release/Options.html

# Changing Directories
setopt AUTO_CD

# Completion
setopt AUTO_LIST
setopt AUTO_MENU
setopt LIST_TYPES

# History
setopt HIST_FCNTL_LOCK
setopt HIST_IGNORE_ALL_DUPS
setopt HIST_REDUCE_BLANKS
setopt SHARE_HISTORY
setopt HIST_BEEP

# Input/Output
setopt CORRECT
